{
  "name": "react-stopwatch",
  "version": "1.0.0",
  "description": "A simple Stopwatch component for ReactJs",
  "main": "dist/index.js",
  "scripts": {
    "flow": "flow",
    "babel": "babel src/ --out-dir dist/",
    "example": "browserify -t babelify --debug example/index.js -o example/bundle.js",
    "build": "yarn run babel && npm run flow && yarn run example",
    "prepublish": "yarn run build",
    "eslint": "eslint ./src && eslint ./example",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/slorenzo/react-stopwatch.git"
  },
  "keywords": [
    "React",
    "ReactJS",
    "Stopwatch"
  ],
  "author": "Sebastian Lorenzo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/slorenzo/react-stopwatch/issues"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/example/"
    ],
    "automock": false,
    "testRegex": "\\.test\\.js$"
  },
  "homepage": "https://github.com/slorenzo/react-stopwatch",
  "peerDependencies": {
    "react": "^15.5.4"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^13.3.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-flow-vars": "^0.5.0",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "flow-bin": "^0.44.1",
    "jest": "^19.0.2",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.5.4"
  }
}
